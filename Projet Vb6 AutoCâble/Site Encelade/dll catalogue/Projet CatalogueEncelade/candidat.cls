Function con_frmField()
          Response.Expires = 0
          Session("CANDIDAT_CURRENTPAGE") = "CON_FRMFIELD.ASP"
  Set Conn = OpenDb(Session("candidat_ADOContact"))

  
        
          If Request("MODE2") = "EXECUTE" Then
                Conn.Execute ("DELETE FROM CON_FIELDS WHERE USERID = " & Session("CANDIDAT_WEB_USERID"))
                STRARRAY = Split(Request("LSTFIELDID"), ",")
                          
                For i = 0 To UBound(STRARRAY)
                        Order = "ORDER" & Trim(STRARRAY(i))
                        FieldOrder = Request(Order)
                        If Not IsNumeric(FieldOrder) Then
                                FieldOrder = 0
                         End If
                        Conn.Execute ("INSERT INTO CON_FIELDS(USERID,FIELDID,FIELDORDER) VALUES(" & Session("CANDIDAT_WEB_USERID") & "," & Trim(STRARRAY(i)) & "," & FieldOrder & ")")
                Next
                '**** REINDEX
                i = 0
                Set rs0 = Conn.Execute("SELECT * FROM CON_FIELDS WHERE USERID = " & Session("CANDIDAT_WEB_USERID") & " ORDER BY FIELDORDER")
                Do While Not rs0.EOF
                        Set RS1 = Conn.Execute("SELECT * FROM CON_FIELDDEFS WHERE FIELDID = " & rs0("FIELDID"))
                        If Not RS1.EOF Then
                                i = i + 1
                                Conn.Execute ("UPDATE CON_FIELDS SET FIELDORDER = " & i & " WHERE USERID = " & Session("CANDIDAT_WEB_USERID") & " AND FIELDID = " & rs0("FIELDID"))
                         End If
                        rs0.MoveNext
                Loop
                msg = "FIELDS SELECTED."
           End If
          


