    If SendEmail2(GetDefault("email", "luis.carreira@encelade.fr", DsnTableMenu), "" & Rs!Mail, GetDefault("ObjetComFour", "Veuillez trouvez ci jointe notre commande. En cas de problème merci de nous le signaler au plus vite.", DsnTableMenu), MailTXT, 0) = False Then GoTo Fin
 End If
Session("MSGCommande") = "Mail Envoy&eacute; le " & Format(Date, "dddd dd mmmm yyyy") & "<br>&agrave; " & Time
