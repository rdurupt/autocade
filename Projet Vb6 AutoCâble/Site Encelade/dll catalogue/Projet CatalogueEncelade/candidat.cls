 If Request("NumerBr") <> "" Then
    If SendEmail2(GetDefault("email", "luis.carreira@encelade.fr", DsnTableMenu), "" & Rs!Mail, GetDefault("ObjetComFourBR", "Veuillez trouvez ci jointe notre Bordereau de réception.", DsnTableMenu), MailTXT, 0) = False Then GoTo Fin
